{
  "name": "nextjs-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next build && next start -p 80",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless",
    "test": "jest",
    "test:watch": "npx test --watch",
    "test:coverage": "npx jest --coverage",
    "lint": "next lint",
    "format": "prettier --config ./.prettierrc --write \"*/**/*{.ts,.tsx}\"",
    "find:unused": "next-unused",
    "install:npm": "rm -rf node_modules && rm package-lock.json && npm i",
    "install:yarn": "rm -rf node_modules && rm yarn.lock && yarn"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "he": "^1.2.0",
    "next": "11.1.3-canary.7",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "slugify": "^1.6.1",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/he": "^1.1.2",
    "@types/jest": "^27.0.2",
    "@types/react": "17.0.30",
    "@types/styled-components": "^5.1.15",
    "babel-jest": "^27.2.5",
    "cypress": "^8.6.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.3-canary.7",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.5",
    "jest-styled-components": "^7.0.5",
    "next-unused": "^0.0.6",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(js|jsx|ts|tsx)'",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint '*/**/*.{ts,tsx}' --fix"
    ]
  },
  "next-unused": {
    "alias": {
      "@components/*": [
        "components/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages",
      "utils"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}